[Desktop Entry]
Type=Application
Name=tr("Dialer")
GenericName=tr("Dialer App")
Comment=tr("Dialer application")
Exec=dialer-app %u
Terminal=false
Icon=phone-app
MimeType=x-scheme-handler/contact;x-scheme-handler/call
X-Ubuntu-Touch=true
X-Ubuntu-StageHint=SideStage
X-Ubuntu-Gettext-Domain=dialer-app
X-Ubuntu-Single-Instance=true
X-Screenshot=@CMAKE_INSTALL_PREFIX@/@DIALER_APP_DIR@/assets/dialer-app-screenshot.png
